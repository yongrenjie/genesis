/* insert pulprog name here */

;$CLASS=HighRes
;$DIM=2D
;$TYPE=
;$SUBTYPE=
;$COMMENT=

#include <Avance.incl>
#include <Grad.incl>
#include <Delay.incl>

/* insert preamble here */

"l0     = td1/(nbl*2)"              ; Total number of t1 increments for 13C HSQC
"acqt0 = 0"
baseopt_echo

1 ze
  30m pl12:f2
2 30m do:f2
3 5m
4 50u UNBLKGRAD
  ; Cleanup
  4u pl2:f2
  (p3 ph1):f2
  4u pl1:f1
  p16:gp0
  4u
  (p1 ph1)
  4u
  p16:gp0*-1.37
  4u
  (p1 ph2)
  4u
  p16:gp0*0.77
  50u BLKGRAD
  d1 st0
  50u UNBLKGRAD

  ; MODULE 1
  /* insert module here */

  ; Purging
  4u pl2:f2
  (p3 ph1):f2
  50u
  p16:gp0*2.32
  2m st

  ; MODULE 2
  /* insert module here */

  ; echo/antiecho loop
  /* increment params with EA here */
  30m wr #0 if #0 zd
  lo to 3 times 2

  ; 13C t1 incrementation loop
  /* increment params with t1 here */
  lo to 4 times l0

50u BLKGRAD
exit 


/* insert pulse phases here */


;gpz0: 17.13%
