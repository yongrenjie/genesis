/* au_zgnoah
 * ---------
 * Runs rga on a pulse-acquire sequence first, then turns on NUS before running
 * the NOAH experiment. To be used as the acquisition AU programme (parameter
 * AUNM) under automation for NUS NOAH supersequences.
 * 
 * v: 2.2.4
 * Jonathan Yong & Tim Claridge, University of Oxford
 * Eriks Kupce, Bruker UK
 * 17 November 2022 */

char pulprog[64];
int nbl;

FETCHPAR("PULPROG", pulprog)
FETCHPAR("NBL", &nbl)
STOREPAR("PULPROG", "zg_rga")
STOREPAR("NBL", 1)
RGA
STOREPAR("PULPROG", pulprog)
STOREPAR("NBL", nbl)
XCMD("xpy noah_nus2 on")
ZG
QUIT
